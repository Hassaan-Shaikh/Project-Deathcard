; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Deathcard"
config/description="The classic game of Rock, Paper, Scissors has been changed into a high stakes game of life and death, using cards and a gun.

Do you have what it takes to best your opponent in this life-threatening game of Rock, Paper, Scissors?"
config/version="0.0.0.1"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/size/resizable=false
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Project Deathcard"

[file_customization]

folder_colors={
"res://Cards/": "teal",
"res://Characters/": "yellow",
"res://Test/": "gray"
}

[rendering]

anti_aliasing/quality/msaa_3d=2
anti_aliasing/quality/screen_space_aa=1
occlusion_culling/use_occlusion_culling=true
